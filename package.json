{
  "name": "reusable-webc",
  "version": "1.32.0",
  "description": "reusable web components",
  "main": "index.js",
  "scripts": {
    "sass": "sass --no-source-map src/components:src/components --watch",
    "dev": "webpack -w --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "serve": "cd dist && http-server -o",
    "start": "npm-run-all --parallel serve dev sass"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rgurgul/reusable-webc.git"
  },
  "keywords": [
    "web components",
    "javascript",
    "binary watch",
    "tabs",
    "radio group"
  ],
  "author": "Robert Gurgul",
  "license": "ISC",
  "devDependencies": {
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "css-loader": "^3.2.0"
  },
  "dependencies": {
    "debugger-look": "^1.0.9",
    "npm-run-all": "^4.1.5",
    "sass": "^1.22.9",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0"
  }
}
